{
    "name": "sinabek/php-skeleton",
    "authors": [
        {
            "name": "SinaBek",
            "email": "me@sinabek.ir",
            "homepage": "https://sinabek.ir",
            "role": "Maintainer"
        }
    ],
    "homepage": "https://github.com/sinabek/php-skeleton/",
    "license": "MIT",
    "description": "An Skeleton PHP application",
    "keywords": ["php", "php-skeleton"],
    "version": "1.0.0",
    "type": "project",
    "readme": "README.md",
    "suggest": {
        "psr/cache": "Common interface for caching libraries",
        "psr/clock": "Common interface for reading the clock",
        "psr/container": "Common Container Interface (PHP FIG PSR-11)",
        "psr/event-dispatcher": "Standard interfaces for event handling",
        "psr/http-client": "Common interface for HTTP clients",
        "psr/http-factory": "PSR-17: Common interfaces for PSR-7 HTTP message factories",
        "psr/http-message": "Common interface for HTTP messages",
        "psr/http-server-handler": "Common interface for HTTP server-side request handler",
        "psr/http-server-middleware": "Common interface for HTTP server-side middleware",
        "psr/link": "Common interfaces for HTTP links",
        "psr/log": "Common interface for logging libraries",
        "psr/simple-cache": "Common interfaces for simple caching"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "prefer-stable": true,
    "config":{
        "process-timeout": 600,
        "allow-plugins": {},
        "use-include-path": false,
        "preferred-install": "dist",
        "secure-http": true,
        "platform":{
            "php": "8.1"
        },
        "bin-compat":"auto",
        "prepend-autoloader": true,
        "autoloader-suffix": "composerautoload",
        "optimize-autoloader": false,
        "sort-packages": true,
        "apcu-autoloader": false,
        "vendor-dir": "vendor",
        "bin-dir": "bin",
        "htaccess-protect":true,
        "platform-check": true
    },
    "scripts": {},
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
